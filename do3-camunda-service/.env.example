# Zeebe gateway (всередині docker-compose: orchestration:26500)
ZEEBE_ADDRESS=orchestration:26500

# URL HTTP-сервісу ДО 3.0 (повний шлях до endpoint)
DO3_URL=https://your-do3-server/do3/hs/CamundaConnector/document

# Job type в BPMN Service Task
JOB_TYPE=do3-document

# Таймаут на виконання одного job (секунди)
JOB_TIMEOUT=30

# Макс. кількість одночасних jobs
MAX_JOBS=10

# Таймаут HTTP-запиту до ДО 3.0 (секунди)
HTTP_TIMEOUT=30
