    <bpmn:sequenceFlow id="Flow_create_main_to_merge" sourceRef="ST_create_main" targetRef="Merge_odoo" />
    <bpmn:sequenceFlow id="Flow_to_detect_features" sourceRef="Merge_odoo" targetRef="ST_detect_features" />
    <bpmn:sequenceFlow id="Flow_features_to_file_type" sourceRef="ST_detect_features" targetRef="XOR_file_type" />
    <bpmn:sequenceFlow id="Flow_excel" name="Excel (xlsx / xls)" sourceRef="XOR_file_type" targetRef="Merge_to_autofill">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">
        = lower case(file_extension) in ["xlsx","xls"]
      </bpmn:conditionExpression>
    </bpmn:sequenceFlow>
