<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:zeebe="http://camunda.org/schema/zeebe/1.0" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Web Modeler" exporterVersion="e787c33" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.8.0">
  <bpmn:collaboration id="Collab_single">
    <bpmn:participant id="Pool_single" name="Оплата окремого рахунку" processRef="Process_single_invoice" />
  </bpmn:collaboration>
  <bpmn:process id="Process_single_invoice" name="Оплата окремого рахунку" isExecutable="true">
    <bpmn:extensionElements>
      <zeebe:versionTag value="1.0" />
    </bpmn:extensionElements>
    <bpmn:laneSet id="LaneSet_1">
      <bpmn:lane id="Lane_Buhgalter" name="Бухгалтер">
        <bpmn:flowNodeRef>Task_pay</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>CA_tp_reissue</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_main</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>StartEvent_1</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>GW_odoo_check</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>GW_odoo_merge</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Camunda" name="Система (Camunda)">
        <bpmn:flowNodeRef>Task_autofill</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>GW_main_split</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>GW_tp_check</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Timer_payday</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>GW_timer_decision</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_dmn_nego_who</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Odoo" name="Система (Odoo авто)">
        <bpmn:flowNodeRef>ST_create_main</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_calendar</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_service_receipt</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Pogodjuvach" name="Погоджувач">
        <bpmn:flowNodeRef>Task_approve</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>GW_approved</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_approver_contract</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>GW_contract_result</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>GW_who_nego</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_dmn_record</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>GW_dmn_decision</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_wait_new</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>CA_break_contract</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_break_contract</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Yulia" name="Юля">
        <bpmn:flowNodeRef>Task_yulia</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>GW_yulia_decision</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_nego_manual</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_nego_manual</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Timer_yulia_deadline</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_no_pay</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Dasha" name="Даша (Менеджер з оренди)">
        <bpmn:flowNodeRef>Task_dasha</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>CA_nego_self</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    <!-- === START + ODOO CHECK === -->
    <bpmn:startEvent id="StartEvent_1" name="Старт">
      <bpmn:outgoing>Flow_start_to_gw_odoo</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:exclusiveGateway id="GW_odoo_check" name="Задача в Odoo існує?" default="Flow_odoo_no_task">
      <bpmn:incoming>Flow_start_to_gw_odoo</bpmn:incoming>
      <bpmn:outgoing>Flow_odoo_no_task</bpmn:outgoing>
      <bpmn:outgoing>Flow_odoo_has_task</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:serviceTask id="ST_create_main" name="Створити задачу рахунку в Odoo">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="http-request-smart" />
        <zeebe:ioMapping>
          <zeebe:input source="= &#34;POST&#34;" target="method" />
          <zeebe:input source="= &#34;https://o.tut.ua/web/hook/8531324a-2785-48d1-8f4d-ddd66a267d50&#34;" target="url" />
          <zeebe:input source="= {&#34;Content-Type&#34;:&#34;application/json&#34;}" target="headers" />
          <zeebe:input source="= {name: x_studio_camunda_invoice_type + &#34;: &#34; + x_studio_camunda_partner_name + &#34; &#34; + string(x_studio_camunda_invoice_amount) + &#34; грн&#34;, create_process: true, _model: &#34;project.project&#34;, _id: 252}" target="body" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_odoo_no_task</bpmn:incoming>
      <bpmn:outgoing>Flow_create_main_done</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:exclusiveGateway id="GW_odoo_merge">
      <bpmn:incoming>Flow_odoo_has_task</bpmn:incoming>
      <bpmn:incoming>Flow_create_main_done</bpmn:incoming>
      <bpmn:outgoing>Flow_odoo_merge_to_autofill</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <!-- === AUTOFILL + MAIN SPLIT === -->
    <bpmn:businessRuleTask id="Task_autofill" name="Автозаповнення документа">
      <bpmn:extensionElements>
        <zeebe:calledDecision decisionId="decision-1goz4yx" resultVariable="docData" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_odoo_merge_to_autofill</bpmn:incoming>
      <bpmn:outgoing>Flow_autofill_to_main_split</bpmn:outgoing>
    </bpmn:businessRuleTask>
    <bpmn:parallelGateway id="GW_main_split">
      <bpmn:incoming>Flow_autofill_to_main_split</bpmn:incoming>
      <bpmn:outgoing>Flow_main_to_calendar</bpmn:outgoing>
      <bpmn:outgoing>Flow_main_to_approve</bpmn:outgoing>
    </bpmn:parallelGateway>
    <!-- === CALENDAR BRANCH === -->
    <bpmn:exclusiveGateway id="GW_tp_check" name="Тип ТП або ТП+?" default="Flow_tp_no">
      <bpmn:incoming>Flow_main_to_calendar</bpmn:incoming>
      <bpmn:outgoing>Flow_tp_yes</bpmn:outgoing>
      <bpmn:outgoing>Flow_tp_no</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:callActivity id="CA_tp_reissue" name="Перевиставлення оренди (ТП/ТП+)">
      <bpmn:extensionElements>
        <zeebe:calledElement propagateAllChildVariables="false" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_tp_yes</bpmn:incoming>
      <bpmn:outgoing>Flow_tp_done</bpmn:outgoing>
    </bpmn:callActivity>
    <bpmn:serviceTask id="Task_calendar" name="Додати суму в платіжний календар">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="http-request-smart" />
        <zeebe:ioMapping>
          <zeebe:input source="= &#34;POST&#34;" target="method" />
          <zeebe:input source="= &#34;https://o.tut.ua/web/hook/8531324a-2785-48d1-8f4d-ddd66a267d50&#34;" target="url" />
          <zeebe:input source="= {&#34;Content-Type&#34;:&#34;application/json&#34;}" target="headers" />
          <zeebe:input source="= {action: &#34;add_to_payment_calendar&#34;, x_studio_camunda_partner_name: x_studio_camunda_partner_name, x_studio_camunda_invoice_amount: x_studio_camunda_invoice_amount, x_studio_camunda_payment_date: x_studio_camunda_payment_date, x_studio_camunda_invoice_type: x_studio_camunda_invoice_type, _model: &#34;project.project&#34;, _id: 252, process_instance_key: process_instance_key}" target="body" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_tp_no</bpmn:incoming>
      <bpmn:incoming>Flow_tp_done</bpmn:incoming>
      <bpmn:outgoing>Flow_calendar_to_timer</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:intermediateCatchEvent id="Timer_payday" name="Очікуємо дедлайну рахунку">
      <bpmn:incoming>Flow_calendar_to_timer</bpmn:incoming>
      <bpmn:outgoing>Flow_timer_to_gw_decision</bpmn:outgoing>
      <bpmn:timerEventDefinition id="TimerDef_payday">
        <bpmn:timeDate xsi:type="bpmn:tFormalExpression">= x_studio_camunda_payment_date</bpmn:timeDate>
      </bpmn:timerEventDefinition>
    </bpmn:intermediateCatchEvent>
    <bpmn:exclusiveGateway id="GW_timer_decision" name="Є питання по рахунку?" default="Flow_timer_to_pay">
      <bpmn:incoming>Flow_timer_to_gw_decision</bpmn:incoming>
      <bpmn:outgoing>Flow_timer_to_pay</bpmn:outgoing>
      <bpmn:outgoing>Flow_timer_to_yulia</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <!-- === APPROVE BRANCH === -->
    <bpmn:userTask id="Task_approve" name="Погодити рахунок">
      <bpmn:extensionElements>
        <zeebe:userTask />
        <zeebe:ioMapping>
          <zeebe:input source="= &#34;POST&#34;" target="method" />
          <zeebe:input source="= &#34;https://o.tut.ua/web/hook/8531324a-2785-48d1-8f4d-ddd66a267d50&#34;" target="url" />
          <zeebe:input source="= {&#34;Content-Type&#34;:&#34;application/json&#34;}" target="headers" />
          <zeebe:input source="= {name: &#34;Погодити рахунок від &#34; + x_studio_camunda_partner_name, description: &#34;Погодьте або відхиліть рахунок на суму &#34; + string(x_studio_camunda_invoice_amount) + &#34; грн від &#34; + x_studio_camunda_partner_name, _model: &#34;project.project&#34;, _id: 252, x_studio_camunda_user_ids: x_studio_camunda_approver_user_id, process_instance_key: process_instance_key}" target="body" />
          <zeebe:output source="= x_studio_camunda_approved" target="x_studio_camunda_approved" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_main_to_approve</bpmn:incoming>
      <bpmn:outgoing>Flow_approve_to_gw</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="GW_approved" name="Погоджено?" default="Flow_approved_no">
      <bpmn:incoming>Flow_approve_to_gw</bpmn:incoming>
      <bpmn:outgoing>Flow_approved_yes</bpmn:outgoing>
      <bpmn:outgoing>Flow_approved_no</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:userTask id="Task_approver_contract" name="Погоджувач з договору">
      <bpmn:extensionElements>
        <zeebe:userTask />
        <zeebe:ioMapping>
          <zeebe:input source="= &#34;POST&#34;" target="method" />
          <zeebe:input source="= &#34;https://o.tut.ua/web/hook/8531324a-2785-48d1-8f4d-ddd66a267d50&#34;" target="url" />
          <zeebe:input source="= {&#34;Content-Type&#34;:&#34;application/json&#34;}" target="headers" />
          <zeebe:input source="= {name: &#34;Перевірка договору: &#34; + x_studio_camunda_partner_name, description: &#34;Перевірте відповідність рахунку умовам договору та погодьте або відхиліть&#34;, _model: &#34;project.project&#34;, _id: 252, x_studio_camunda_user_ids: x_studio_camunda_approver_user_id, process_instance_key: process_instance_key}" target="body" />
          <zeebe:output source="= x_studio_camunda_contract_approved" target="x_studio_camunda_contract_approved" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_approved_yes</bpmn:incoming>
      <bpmn:outgoing>Flow_contract_to_gw</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="GW_contract_result" name="Погоджено з договору?" default="Flow_contract_no">
      <bpmn:incoming>Flow_contract_to_gw</bpmn:incoming>
      <bpmn:outgoing>Flow_contract_yes</bpmn:outgoing>
      <bpmn:outgoing>Flow_contract_no</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <!-- === NEGO BRANCH === -->
    <bpmn:businessRuleTask id="Task_dmn_nego_who" name="Визначити виконавця перемовин">
      <bpmn:extensionElements>
        <zeebe:calledDecision decisionId="nego-who" resultVariable="x_studio_camunda_nego_who" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_contract_no</bpmn:incoming>
      <bpmn:outgoing>Flow_dmn_nego_who_to_gw</bpmn:outgoing>
    </bpmn:businessRuleTask>
    <bpmn:exclusiveGateway id="GW_who_nego" name="Хто веде переговори?">
      <bpmn:incoming>Flow_dmn_nego_who_to_gw</bpmn:incoming>
      <bpmn:outgoing>Flow_nego_dasha</bpmn:outgoing>
      <bpmn:outgoing>Flow_nego_self</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:userTask id="Task_nego_manual" name="Ведемо перемовини з ТРЦ">
      <bpmn:extensionElements>
        <zeebe:userTask />
        <zeebe:ioMapping>
          <zeebe:input source="= &#34;POST&#34;" target="method" />
          <zeebe:input source="= &#34;https://o.tut.ua/web/hook/8531324a-2785-48d1-8f4d-ddd66a267d50&#34;" target="url" />
          <zeebe:input source="= {&#34;Content-Type&#34;:&#34;application/json&#34;}" target="headers" />
          <zeebe:input source="= {name: &#34;Переговори з &#34; + x_studio_camunda_partner_name, description: &#34;Проведіть переговори щодо умов або зменшення суми рахунку&#34;, _model: &#34;project.project&#34;, _id: 252, x_studio_camunda_user_ids: x_studio_camunda_dasha_user_id, process_instance_key: process_instance_key}" target="body" />
          <zeebe:output source="= x_studio_camunda_nego_outcome" target="x_studio_camunda_nego_outcome" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_approved_no</bpmn:incoming>
      <bpmn:incoming>Flow_nego_dasha</bpmn:incoming>
      <bpmn:outgoing>Flow_nego_manual_end</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:endEvent id="End_nego_manual" name="Переговори завершено">
      <bpmn:incoming>Flow_nego_manual_end</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:callActivity id="CA_nego_self" name="Перемовини з ТРЦ">
      <bpmn:extensionElements>
        <zeebe:calledElement propagateAllChildVariables="false" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_nego_self</bpmn:incoming>
      <bpmn:outgoing>Flow_nego_self_to_gw</bpmn:outgoing>
    </bpmn:callActivity>
    <bpmn:businessRuleTask id="Task_dmn_record" name="Визначити результат перемовин">
      <bpmn:extensionElements>
        <zeebe:calledDecision decisionId="nego-result" resultVariable="nego_action" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_nego_self_to_gw</bpmn:incoming>
      <bpmn:outgoing>Flow_dmn_to_gw</bpmn:outgoing>
    </bpmn:businessRuleTask>
    <bpmn:exclusiveGateway id="GW_dmn_decision" name="Результат перемовин?" default="Flow_dmn_pay">
      <bpmn:incoming>Flow_dmn_to_gw</bpmn:incoming>
      <bpmn:outgoing>Flow_dmn_pay</bpmn:outgoing>
      <bpmn:outgoing>Flow_dmn_wait</bpmn:outgoing>
      <bpmn:outgoing>Flow_dmn_break</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:endEvent id="End_wait_new" name="Чекаємо новий рахунок">
      <bpmn:incoming>Flow_dmn_wait</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:callActivity id="CA_break_contract" name="Розриваємо договір">
      <bpmn:extensionElements>
        <zeebe:calledElement propagateAllChildVariables="false" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_dmn_break</bpmn:incoming>
      <bpmn:outgoing>Flow_break_to_end</bpmn:outgoing>
    </bpmn:callActivity>
    <bpmn:endEvent id="End_break_contract" name="Договір розірвано">
      <bpmn:incoming>Flow_break_to_end</bpmn:incoming>
    </bpmn:endEvent>
    <!-- === YULIA / DASHA ESCALATION === -->
    <bpmn:userTask id="Task_yulia" name="Юля, ми не домовилися по рахунку — платимо?">
      <bpmn:extensionElements>
        <zeebe:userTask />
        <zeebe:ioMapping>
          <zeebe:input source="= &#34;POST&#34;" target="method" />
          <zeebe:input source="= &#34;https://o.tut.ua/web/hook/8531324a-2785-48d1-8f4d-ddd66a267d50&#34;" target="url" />
          <zeebe:input source="= {&#34;Content-Type&#34;:&#34;application/json&#34;}" target="headers" />
          <zeebe:input source="= {name: &#34;Рішення по рахунку: &#34; + x_studio_camunda_partner_name, description: &#34;Дедлайн наближається. Рахунок на суму &#34; + string(x_studio_camunda_invoice_amount) + &#34; грн. Підтвердьте оплату або відхиліть&#34;, _model: &#34;project.project&#34;, _id: 252, x_studio_camunda_user_ids: x_studio_camunda_yulia_user_id, process_instance_key: process_instance_key}" target="body" />
          <zeebe:output source="= x_studio_camunda_pay" target="x_studio_camunda_pay" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_timer_to_yulia</bpmn:incoming>
      <bpmn:outgoing>Flow_yulia_to_gw</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:boundaryEvent id="Timer_yulia_deadline" name="Дедлайн" attachedToRef="Task_yulia">
      <bpmn:outgoing>Flow_yulia_timer_to_dasha</bpmn:outgoing>
      <bpmn:timerEventDefinition id="TimerDef_yulia">
        <bpmn:timeDuration xsi:type="bpmn:tFormalExpression">P1D</bpmn:timeDuration>
      </bpmn:timerEventDefinition>
    </bpmn:boundaryEvent>
    <bpmn:exclusiveGateway id="GW_yulia_decision" name="Платимо?" default="Flow_yulia_no">
      <bpmn:incoming>Flow_yulia_to_gw</bpmn:incoming>
      <bpmn:outgoing>Flow_yulia_yes</bpmn:outgoing>
      <bpmn:outgoing>Flow_yulia_no</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:endEvent id="End_no_pay" name="Рахунок не сплачено">
      <bpmn:incoming>Flow_yulia_no</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:userTask id="Task_dasha" name="Даша, виріши з оплатою рахунку">
      <bpmn:extensionElements>
        <zeebe:userTask />
        <zeebe:ioMapping>
          <zeebe:input source="= &#34;POST&#34;" target="method" />
          <zeebe:input source="= &#34;https://o.tut.ua/web/hook/8531324a-2785-48d1-8f4d-ddd66a267d50&#34;" target="url" />
          <zeebe:input source="= {&#34;Content-Type&#34;:&#34;application/json&#34;}" target="headers" />
          <zeebe:input source="= {name: &#34;Ескалація: оплата рахунку &#34; + x_studio_camunda_partner_name, description: &#34;Юля не прийняла рішення вчасно. Рахунок &#34; + string(x_studio_camunda_invoice_amount) + &#34; грн. Прийміть рішення щодо оплати&#34;, _model: &#34;project.project&#34;, _id: 252, x_studio_camunda_user_ids: x_studio_camunda_dasha_user_id, process_instance_key: process_instance_key}" target="body" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_yulia_timer_to_dasha</bpmn:incoming>
      <bpmn:outgoing>Flow_dasha_to_pay</bpmn:outgoing>
    </bpmn:userTask>
    <!-- === PAY + SERVICE RECEIPT === -->
    <bpmn:userTask id="Task_pay" name="Оплатити рахунок">
      <bpmn:extensionElements>
        <zeebe:userTask />
        <zeebe:ioMapping>
          <zeebe:input source="= &#34;POST&#34;" target="method" />
          <zeebe:input source="= &#34;https://o.tut.ua/web/hook/8531324a-2785-48d1-8f4d-ddd66a267d50&#34;" target="url" />
          <zeebe:input source="= {&#34;Content-Type&#34;:&#34;application/json&#34;}" target="headers" />
          <zeebe:input source="= {name: &#34;Оплатити рахунок: &#34; + x_studio_camunda_partner_name, description: &#34;Виконайте оплату рахунку на суму &#34; + string(x_studio_camunda_invoice_amount) + &#34; грн. Підтвердьте після оплати&#34;, _model: &#34;project.project&#34;, _id: 252, x_studio_camunda_user_ids: x_studio_camunda_buhgalter_user_id, process_instance_key: process_instance_key}" target="body" />
