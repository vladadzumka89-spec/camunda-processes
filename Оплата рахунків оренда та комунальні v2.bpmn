<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions
  xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
  xmlns:zeebe="http://camunda.org/schema/zeebe/1.0"
  xmlns:modeler="http://camunda.org/schema/modeler/1.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  id="Definitions_1"
  targetNamespace="http://bpmn.io/schema/bpmn"
  exporter="Camunda Web Modeler"
  exporterVersion="e787c33"
  modeler:executionPlatform="Camunda Cloud"
  modeler:executionPlatformVersion="8.8.0">

  <!-- ============================================================ -->
  <!-- COLLABORATION (Pool)                                          -->
  <!-- ============================================================ -->
  <bpmn:collaboration id="Collab_1">
    <bpmn:participant id="Pool_1" name="Оплата рахунків оренда та комунальні" processRef="Process_1" />
  </bpmn:collaboration>

  <!-- ============================================================ -->
  <!-- PROCESS                                                       -->
  <!-- ============================================================ -->
  <bpmn:process id="Process_1" name="Оплата рахунків оренда та комунальні" isExecutable="true">
    <bpmn:extensionElements>
      <zeebe:versionTag value="2.0" />
    </bpmn:extensionElements>

    <!-- ========================================================== -->
    <!-- LANES                                                        -->
    <!--                                                              -->
    <!-- Розподіл по лейнах:                                         -->
    <!--   Бухгалтер   — введення файлу, оплата, кінець              -->
    <!--   Система     — DMN, календар, таймер, поступлення          -->
    <!--   Погоджувач  — погодження рахунку та договору              -->
    <!--   Юля         — вирішення питань по рахунку                 -->
    <!--   Даша        — менеджер з оренди: переговини та ескалація  -->
    <!-- ========================================================== -->
    <bpmn:laneSet id="LaneSet_1">

      <bpmn:lane id="Lane_Buhgalter" name="Бухгалтер">
        <bpmn:flowNodeRef>StartEvent_1</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>GW_intro_split</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_add_file</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_detail</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>GW_intro_merge</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_pay</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_main</bpmn:flowNodeRef>
      </bpmn:lane>

      <bpmn:lane id="Lane_Systema" name="Система (авто)">
        <bpmn:flowNodeRef>Task_autofill</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>GW_main_split</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_calendar</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Timer_payday</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>GW_timer_decision</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_service_receipt</bpmn:flowNodeRef>
      </bpmn:lane>

      <bpmn:lane id="Lane_Pogodjuvach" name="Погоджувач">
        <bpmn:flowNodeRef>Task_approve</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>GW_approved</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_approver_contract</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>GW_contract_result</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>GW_who_nego</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_dmn_record</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>GW_dmn_decision</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>CA_break_contract</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_break_contract</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_wait_new</bpmn:flowNodeRef>
      </bpmn:lane>

      <!-- Юля — вирішує питання по рахунку + веде переговини з ТРЦ сама -->
      <bpmn:lane id="Lane_Yulia" name="Юля">
        <bpmn:flowNodeRef>Task_yulia</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Timer_yulia_deadline</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>GW_yulia_decision</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_no_pay</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>CA_nego_self</bpmn:flowNodeRef>
      </bpmn:lane>

      <!-- Даша = Менеджер з оренди: переговини з ТРЦ + ескалація оплати -->
      <bpmn:lane id="Lane_Dasha" name="Даша (Менеджер з оренди)">
        <bpmn:flowNodeRef>Task_nego_manual</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_nego_manual</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_dasha</bpmn:flowNodeRef>
      </bpmn:lane>

    </bpmn:laneSet>

    <!-- ========================================================== -->
    <!-- БЛОК 1: СТАРТ + введення даних (паралельно)                -->
    <!-- ========================================================== -->

    <bpmn:startEvent id="StartEvent_1" name="Старт">
      <bpmn:outgoing>Flow_start_to_gw_intro</bpmn:outgoing>
    </bpmn:startEvent>

    <bpmn:parallelGateway id="GW_intro_split">
      <bpmn:incoming>Flow_start_to_gw_intro</bpmn:incoming>
      <bpmn:outgoing>Flow_split_to_add_file</bpmn:outgoing>
      <bpmn:outgoing>Flow_split_to_detail</bpmn:outgoing>
    </bpmn:parallelGateway>

    <bpmn:userTask id="Task_add_file" name="Додати файл з рахунком">
      <bpmn:extensionElements>
        <zeebe:userTask />
        <zeebe:ioMapping>
          <zeebe:output target="invoice_file" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_split_to_add_file</bpmn:incoming>
      <bpmn:outgoing>Flow_add_file_to_merge</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:userTask id="Task_detail" name="Деталізація рахунку">
      <bpmn:extensionElements>
        <zeebe:userTask />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_split_to_detail</bpmn:incoming>
      <bpmn:outgoing>Flow_detail_to_merge</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:parallelGateway id="GW_intro_merge">
      <bpmn:incoming>Flow_add_file_to_merge</bpmn:incoming>
      <bpmn:incoming>Flow_detail_to_merge</bpmn:incoming>
      <bpmn:outgoing>Flow_merge_to_autofill</bpmn:outgoing>
    </bpmn:parallelGateway>

    <!-- ========================================================== -->
    <!-- БЛОК 2: АВТОЗАПОВНЕННЯ (DMN)                               -->
    <!-- ========================================================== -->

    <bpmn:businessRuleTask id="Task_autofill" name="Автозаповнення документа">
      <bpmn:extensionElements>
        <zeebe:calledDecision decisionId="decision-1goz4yx" resultVariable="docData" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_merge_to_autofill</bpmn:incoming>
      <bpmn:outgoing>Flow_autofill_to_main_split</bpmn:outgoing>
    </bpmn:businessRuleTask>

    <!-- ========================================================== -->
    <!-- БЛОК 3: ПАРАЛЕЛЬ → платіжний календар + погодження         -->
    <!-- ========================================================== -->

    <bpmn:parallelGateway id="GW_main_split">
      <bpmn:incoming>Flow_autofill_to_main_split</bpmn:incoming>
      <bpmn:outgoing>Flow_main_to_calendar</bpmn:outgoing>
      <bpmn:outgoing>Flow_main_to_approve</bpmn:outgoing>
    </bpmn:parallelGateway>

    <bpmn:serviceTask id="Task_calendar" name="Додати суму в платіжний календар">
      <bpmn:incoming>Flow_main_to_calendar</bpmn:incoming>
      <bpmn:outgoing>Flow_calendar_to_timer</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:intermediateCatchEvent id="Timer_payday" name="Очікуємо дедлайну рахунку">
      <bpmn:incoming>Flow_calendar_to_timer</bpmn:incoming>
      <bpmn:outgoing>Flow_timer_to_gw_decision</bpmn:outgoing>
      <bpmn:timerEventDefinition id="TimerDef_payday">
        <bpmn:timeDate xsi:type="bpmn:tFormalExpression">= x_studio_camunda_payment_date</bpmn:timeDate>
      </bpmn:timerEventDefinition>
    </bpmn:intermediateCatchEvent>

    <!-- XOR: після таймера — є питання? -->
    <bpmn:exclusiveGateway id="GW_timer_decision" name="Є питання по рахунку?" default="Flow_timer_to_pay">
      <bpmn:incoming>Flow_timer_to_gw_decision</bpmn:incoming>
      <bpmn:outgoing>Flow_timer_to_pay</bpmn:outgoing>
      <bpmn:outgoing>Flow_timer_to_yulia</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <!-- ========================================================== -->
    <!-- БЛОК 4: ПОГОДЖЕННЯ РАХУНКУ (Погоджувач)                    -->
    <!-- ========================================================== -->

    <bpmn:userTask id="Task_approve" name="Погодити рахунок">
      <bpmn:extensionElements>
        <zeebe:userTask />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_main_to_approve</bpmn:incoming>
      <bpmn:outgoing>Flow_approve_to_gw</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:exclusiveGateway id="GW_approved" name="Погоджено?" default="Flow_approved_no">
      <bpmn:incoming>Flow_approve_to_gw</bpmn:incoming>
      <bpmn:outgoing>Flow_approved_yes</bpmn:outgoing>
      <bpmn:outgoing>Flow_approved_no</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <!-- ========================================================== -->
    <!-- БЛОК 5: ПОГОДЖУВАЧ З ДОГОВОРУ (Погоджувач)                 -->
    <!-- ========================================================== -->

    <bpmn:userTask id="Task_approver_contract" name="Погоджувач з договору">
      <bpmn:extensionElements>
        <zeebe:userTask />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_approved_yes</bpmn:incoming>
      <bpmn:outgoing>Flow_contract_to_gw</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:exclusiveGateway id="GW_contract_result" name="Погоджено з договору?" default="Flow_contract_no">
      <bpmn:incoming>Flow_contract_to_gw</bpmn:incoming>
      <bpmn:outgoing>Flow_contract_yes</bpmn:outgoing>
      <bpmn:outgoing>Flow_contract_no</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <!-- ========================================================== -->
    <!-- БЛОК 6: ПЕРЕГОВИНИ (Даша = Менеджер з оренди)              -->
    <!-- ========================================================== -->

    <bpmn:exclusiveGateway id="GW_who_nego" name="Хто веде переговини?">
      <bpmn:incoming>Flow_contract_no</bpmn:incoming>
      <bpmn:outgoing>Flow_nego_dasha</bpmn:outgoing>
      <bpmn:outgoing>Flow_nego_self</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <!-- Даша веде переговини вручну -->
    <bpmn:userTask id="Task_nego_manual" name="Ведемо перемовини з ТРЦ">
      <bpmn:extensionElements>
        <zeebe:userTask />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_approved_no</bpmn:incoming>
      <bpmn:incoming>Flow_nego_dasha</bpmn:incoming>
      <bpmn:outgoing>Flow_nego_manual_end</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:endEvent id="End_nego_manual" name="Переговини завершено">
      <bpmn:incoming>Flow_nego_manual_end</bpmn:incoming>
    </bpmn:endEvent>

    <!-- Даша веде переговини через підпроцес -->
    <bpmn:callActivity id="CA_nego_self" name="Перемовини з ТРЦ">
      <bpmn:extensionElements>
        <zeebe:calledElement propagateAllChildVariables="false" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_nego_self</bpmn:incoming>
      <bpmn:outgoing>Flow_nego_self_to_gw</bpmn:outgoing>
    </bpmn:callActivity>

    <!-- Визначити результат перемовин (DMN) — Система -->
    <bpmn:businessRuleTask id="Task_dmn_record" name="Визначити результат перемовин">
      <bpmn:extensionElements>
        <zeebe:calledDecision decisionId="nego-result" resultVariable="nego_action" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_nego_self_to_gw</bpmn:incoming>
      <bpmn:outgoing>Flow_dmn_to_gw</bpmn:outgoing>
    </bpmn:businessRuleTask>

    <bpmn:exclusiveGateway id="GW_dmn_decision" name="Результат перемовин?" default="Flow_dmn_pay">
      <bpmn:incoming>Flow_dmn_to_gw</bpmn:incoming>
      <bpmn:outgoing>Flow_dmn_pay</bpmn:outgoing>
      <bpmn:outgoing>Flow_dmn_wait</bpmn:outgoing>
      <bpmn:outgoing>Flow_dmn_break</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:callActivity id="CA_break_contract" name="Розриваємо договір">
      <bpmn:extensionElements>
        <zeebe:calledElement propagateAllChildVariables="false" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_dmn_break</bpmn:incoming>
      <bpmn:outgoing>Flow_break_to_end</bpmn:outgoing>
    </bpmn:callActivity>

    <bpmn:endEvent id="End_break_contract" name="Договір розірвано">
      <bpmn:incoming>Flow_break_to_end</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:endEvent id="End_wait_new" name="Чекаємо новий рахунок">
      <bpmn:incoming>Flow_dmn_wait</bpmn:incoming>
    </bpmn:endEvent>

    <!-- ========================================================== -->
    <!-- БЛОК 7: ЮЛЯ — вирішує питання по рахунку                  -->
    <!-- ========================================================== -->

    <!-- Юля відповідає на питання (з boundary timer → ескалація до Даші) -->
    <bpmn:userTask id="Task_yulia" name="Юля, ми не домовилися по рахунку — платимо?">
      <bpmn:extensionElements>
        <zeebe:userTask />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_timer_to_yulia</bpmn:incoming>
      <bpmn:outgoing>Flow_yulia_to_gw</bpmn:outgoing>
    </bpmn:userTask>

    <!-- Boundary Timer (interrupting) — якщо Юля не відповіла → Даша -->
    <bpmn:boundaryEvent id="Timer_yulia_deadline" name="Дедлайн" attachedToRef="Task_yulia">
      <bpmn:outgoing>Flow_yulia_timer_to_dasha</bpmn:outgoing>
      <bpmn:timerEventDefinition id="TimerDef_yulia">
        <bpmn:timeDuration xsi:type="bpmn:tFormalExpression">P1D</bpmn:timeDuration>
      </bpmn:timerEventDefinition>
    </bpmn:boundaryEvent>

    <bpmn:exclusiveGateway id="GW_yulia_decision" name="Платимо?" default="Flow_yulia_no">
      <bpmn:incoming>Flow_yulia_to_gw</bpmn:incoming>
      <bpmn:outgoing>Flow_yulia_yes</bpmn:outgoing>
      <bpmn:outgoing>Flow_yulia_no</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:endEvent id="End_no_pay" name="Рахунок не сплачено">
      <bpmn:incoming>Flow_yulia_no</bpmn:incoming>
    </bpmn:endEvent>

    <!-- ========================================================== -->
    <!-- БЛОК 8: ДАША — ескалація оплати                            -->
    <!-- ========================================================== -->

    <!-- Даша вирішує оплату після ескалації від Юлі -->
    <bpmn:userTask id="Task_dasha" name="Даша, виріши з оплатою рахунку">
      <bpmn:extensionElements>
        <zeebe:userTask />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_yulia_timer_to_dasha</bpmn:incoming>
      <bpmn:outgoing>Flow_dasha_to_pay</bpmn:outgoing>
    </bpmn:userTask>

    <!-- ========================================================== -->
    <!-- БЛОК 9: ОПЛАТА ТА ЗАКРИТТЯ (Бухгалтер + Система)          -->
    <!-- ========================================================== -->

    <bpmn:userTask id="Task_pay" name="Оплатити рахунок">
      <bpmn:extensionElements>
        <zeebe:userTask />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_timer_to_pay</bpmn:incoming>
      <bpmn:incoming>Flow_contract_yes</bpmn:incoming>
      <bpmn:incoming>Flow_dmn_pay</bpmn:incoming>
      <bpmn:incoming>Flow_yulia_yes</bpmn:incoming>
      <bpmn:incoming>Flow_dasha_to_pay</bpmn:incoming>
      <bpmn:outgoing>Flow_pay_to_service</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:serviceTask id="Task_service_receipt" name="Поступлення послуг на основі рахунку">
      <bpmn:incoming>Flow_pay_to_service</bpmn:incoming>
      <bpmn:outgoing>Flow_service_to_end</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:endEvent id="End_main" name="Рахунок оплачено">
      <bpmn:incoming>Flow_service_to_end</bpmn:incoming>
    </bpmn:endEvent>

    <!-- ========================================================== -->
    <!-- SEQUENCE FLOWS                                              -->
    <!-- ========================================================== -->

    <bpmn:sequenceFlow id="Flow_start_to_gw_intro"      sourceRef="StartEvent_1"          targetRef="GW_intro_split" />
    <bpmn:sequenceFlow id="Flow_split_to_add_file"      sourceRef="GW_intro_split"         targetRef="Task_add_file" />
    <bpmn:sequenceFlow id="Flow_split_to_detail"        sourceRef="GW_intro_split"         targetRef="Task_detail" />
    <bpmn:sequenceFlow id="Flow_add_file_to_merge"      sourceRef="Task_add_file"          targetRef="GW_intro_merge" />
    <bpmn:sequenceFlow id="Flow_detail_to_merge"        sourceRef="Task_detail"            targetRef="GW_intro_merge" />
    <bpmn:sequenceFlow id="Flow_merge_to_autofill"      sourceRef="GW_intro_merge"         targetRef="Task_autofill" />
    <bpmn:sequenceFlow id="Flow_autofill_to_main_split" sourceRef="Task_autofill"          targetRef="GW_main_split" />
    <bpmn:sequenceFlow id="Flow_main_to_calendar"       sourceRef="GW_main_split"          targetRef="Task_calendar" />
    <bpmn:sequenceFlow id="Flow_main_to_approve"        sourceRef="GW_main_split"          targetRef="Task_approve" />
    <bpmn:sequenceFlow id="Flow_calendar_to_timer"      sourceRef="Task_calendar"          targetRef="Timer_payday" />
    <bpmn:sequenceFlow id="Flow_timer_to_gw_decision"   sourceRef="Timer_payday"           targetRef="GW_timer_decision" />
    <bpmn:sequenceFlow id="Flow_timer_to_pay"           sourceRef="GW_timer_decision"      targetRef="Task_pay" />
    <bpmn:sequenceFlow id="Flow_timer_to_yulia"         sourceRef="GW_timer_decision"      targetRef="Task_yulia">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">= x_studio_camunda_has_question = true</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_approve_to_gw"          sourceRef="Task_approve"           targetRef="GW_approved" />
    <bpmn:sequenceFlow id="Flow_approved_yes"           sourceRef="GW_approved"            targetRef="Task_approver_contract" name="так">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">= x_studio_camunda_approved = true</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_approved_no"            sourceRef="GW_approved"            targetRef="Task_nego_manual" name="ні" />
    <bpmn:sequenceFlow id="Flow_contract_to_gw"         sourceRef="Task_approver_contract" targetRef="GW_contract_result" />
    <bpmn:sequenceFlow id="Flow_contract_yes"           sourceRef="GW_contract_result"     targetRef="Task_pay" name="так">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">= x_studio_camunda_contract_approved = true</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_contract_no"            sourceRef="GW_contract_result"     targetRef="GW_who_nego" name="ні" />
    <bpmn:sequenceFlow id="Flow_nego_dasha"             sourceRef="GW_who_nego"            targetRef="Task_nego_manual" name="Веде переговини Даша">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">= x_studio_camunda_nego_who = "dasha"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_nego_self"              sourceRef="GW_who_nego"            targetRef="CA_nego_self" name="Веде переговини Юля">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">= x_studio_camunda_nego_who = "yulia"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_nego_manual_end"        sourceRef="Task_nego_manual"       targetRef="End_nego_manual" />
    <bpmn:sequenceFlow id="Flow_nego_self_to_gw"        sourceRef="CA_nego_self"           targetRef="Task_dmn_record" />
    <bpmn:sequenceFlow id="Flow_dmn_to_gw"             sourceRef="Task_dmn_record"        targetRef="GW_dmn_decision" />
    <bpmn:sequenceFlow id="Flow_dmn_pay"               sourceRef="GW_dmn_decision"        targetRef="Task_pay" name="Платимо" />
    <bpmn:sequenceFlow id="Flow_dmn_wait"              sourceRef="GW_dmn_decision"        targetRef="End_wait_new" name="Перевипуск рахунку">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">= nego_action = "reissue"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_dmn_break"             sourceRef="GW_dmn_decision"        targetRef="CA_break_contract" name="Розриваємо договір">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">= nego_action = "break_contract"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_break_to_end"          sourceRef="CA_break_contract"      targetRef="End_break_contract" />
    <bpmn:sequenceFlow id="Flow_yulia_to_gw"           sourceRef="Task_yulia"             targetRef="GW_yulia_decision" />
    <bpmn:sequenceFlow id="Flow_yulia_timer_to_dasha"  sourceRef="Timer_yulia_deadline"   targetRef="Task_dasha" />
