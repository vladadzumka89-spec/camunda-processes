      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">
        = processingFeatures.processing_method = "ai_assistant"
      </bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_manual_no_codes_path" name="Без кодів" sourceRef="XOR_processing" targetRef="UT_manual_no_codes">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">
