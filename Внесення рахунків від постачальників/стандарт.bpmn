      <bpmn:incoming>Flow_to_xor_recognized</bpmn:incoming>
      <bpmn:outgoing>Flow_recognized</bpmn:outgoing>
      <bpmn:outgoing>Flow_not_recognized</bpmn:outgoing>
    </bpmn:exclusiveGateway>
